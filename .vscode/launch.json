{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vite Dev (Browser)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/*"
            },
            "preLaunchTask": "npm: dev"
        }
    ]
}
